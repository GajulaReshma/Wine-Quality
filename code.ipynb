    "# coding: utf-8\n",
    "# NOTE THIS SCRIPT IS WRITTEN FOR python2.7\n",
    "#### ipynb file\n",
    "\n",
    "import sys\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.neighbors import NearestNeighbors\n",
    "from sklearn import linear_model\n",
    "from sklearn import svm\n",
    "from sklearn import naive_bayes\n",
    "from sklearn import neighbors\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Assumes inputs are pandas data frames\n",
    "# Assumes the last column of data is the output dimension\n",
    "\n",
    "d = open(sys.argv[1],'r')\n",
    "num_folds = int(sys.argv[2])\n",
    "model_name = sys.argv[3]\n",
    "Data = pd.read_csv(d)\n",
    "n,m = Data.shape\n",
    "\n",
    "\n",
    "#coverting output column to binary format\n",
    "last_col = Data[Data.columns[-1]]\n",
    "uniq,freq = np.unique(last_col,return_counts=True)\n",
    "\n",
    "w = np.argsort(-freq) \n",
    "uniq1 = uniq[w]\n",
    "freq1 = freq[w]\n",
    "Data[Data.columns[-1]] = np.where(Data[Data.columns[-1]] == uniq1[0], 1, 0)\n"
