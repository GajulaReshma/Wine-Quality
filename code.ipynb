    "##############################\n",
    "# PART 3\n",
    "print '\\n ############ PART 3 ############# \\n'\n",
    "##############################\n",
    "\n",
    "\n",
    "#input prediction file the first column of which is prediction value\n",
    "#the 2nd column is true label (0/1)\n",
    "#cutoff is a numeric value, default is 0.5\n",
    "def get_metrics(pred, cutoff=0.5):\n",
    "    TP = 0\n",
    "    FP = 0\n",
    "    TN = 0\n",
    "    FN = 0\n",
    "    \n",
    "    prediction = pred[pred.columns[0]]\n",
    "    true_output = pred[pred.columns[1]]\n",
    "    \n",
    "    prediction = np.where(prediction >= cutoff, 1,0) #replacing probabilities with predicted values\n",
    "        \n",
    "    for i in range(len(true_output)): \n",
    "        if true_output[i]==prediction[i]==1: #prdicted true (1) for positive value(1)\n",
    "           TP += 1\n",
    "        if true_output[i]==0 and true_output[i]!=prediction[i]: #prdicted false (0) for positive value(1)\n",
    "           FP += 1\n",
    "        if true_output[i]==prediction[i]==0: #prdicted false (0) for negative value(0)\n",
    "           TN += 1\n",
    "        if true_output[i]==1 and true_output[i]!=prediction[i]: #prdicted ture (1) for negative value(0)\n",
    "           FN += 1\n",
    "        \n",
    "    N = TP+TN+FP+FN\n",
    "    TP_rate = TP/float(TP+FN) #or recall\n",
    "    FP_rate = FP/float(TN+FP)\n",
    "    precision = TP/float(TP+FP)\n",
    "    accuracy = (TP+TN)/float(N)\n",
    "    \n",
    "    DF = pd.DataFrame({'True Positive Rate':[TP_rate],'False Positive Rate':[FP_rate],'Accuracy':[accuracy],'Precision':[precision],'Recall':[TP_rate]}).round(2)\n",
    "    return DF\n",
    "\n",
    "####################\n",
    "####import data#####\n",
    "print '\\n ############ Import data ############# \\n'\n",
    "####################\n",
    "\n",
    "my_data = pd.read_csv('wine.csv')\n",
