    "##############################\n",
    "# PART 2\n",
    "print '\\n ############ PART 2 ############# \\n'\n",
    "##############################\n",
    "\n",
    "#your implementation of do_cv_class goes here\n",
    "def do_cv_class(df, num_folds, model_name):\n",
    "    n,m = df.shape\n",
    "    \n",
    "    shuf = df.sample(frac=1,random_state=1).reset_index(drop=True) #shuffle rows in a dataframe\n",
    "    \n",
    "    OUT = []\n",
    "    fold = []\n",
    "    #num_folds-fold cross validation\n",
    "    for i in range(num_folds):      \n",
    "        splits = np.array_split(shuf,num_folds)\n",
    "        splits_in_list = list(splits)\n",
    "        test = splits_in_list.pop(i)\n",
    "\n",
    "        train = pd.concat(splits_in_list,axis=0)\n",
    "    \n",
    "        if model_name == 'knn':\n",
    "            k = int(model_name.split('nn')[0])\n",
    "            out = get_pred_knn(train,test,k)\n",
    "        elif model_name == 'logreg':\n",
    "            out = get_pred_logreg(train,test)\n",
    "        elif model_name == 'svm':\n",
    "            out = get_pred_svm(train,test)        \n",
    "        else:\n",
    "            out = get_pred_nb(train,test)\n",
    "        \n",
    "        out = out.reset_index(drop=True)\n",
    "        for index,row in out.iterrows():\n",
    "            fold.append(i)\n",
    "        \n",
    "        \n",
    "        #join all data frames for each i\n",
    "        OUT.append(out)\n",
    "    OUTPUT_DF = pd.concat(OUT,ignore_index=False).reset_index(drop=True)\n",
    "    fold = [a+1 for a in fold]\n",
    "    OUTPUT_DF['fold'] = fold\n",
    "    return OUTPUT_DF\n",
    "\n"
   ]
