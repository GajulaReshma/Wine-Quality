    "##############################\n",
    "# PART 1\n",
    "print '\\n ############ PART 1 ############# \\n'\n",
    "##############################\n",
    "\n",
    "# Logistic Regression\n",
    "# Assumes the last column of data is the output dimension\n",
    "def get_pred_logreg(train,test):\n",
    "    n,m = train.shape \n",
    "    X_train = train.iloc[:,:m-1] # get training input data\n",
    "    X_test = test.iloc[:,:m-1] # get test input data\n",
    "    Y_train = train.iloc[:,m-1] # get train output data\n",
    "    Y_test = test.iloc[:,m-1] # get test output data\n",
    "    clf = linear_model.LogisticRegression(C=1e5)\n",
    "    clf.fit(X_train, Y_train) \n",
    "    pred = clf.predict_proba(X_test)[:,1]#returns probability estimate of class 1\n",
    "    DF = pd.DataFrame({'prediction': pred, 'true output': Y_test})\n",
    "    return DF\n",
    "\n",
    "# Support Vector Machine\n",
    "# Assumes the last column of data is the output dimension\n",
    "def get_pred_svm(train,test):\n",
    "    n,m = train.shape \n",
    "    X_train = train.iloc[:,:m-1] # get training input data\n",
    "    X_test = test.iloc[:,:m-1] # get test input data\n",
    "    Y_train = train.iloc[:,m-1] # get train output data\n",
    "    Y_test = test.iloc[:,m-1] # get test output data\n",
    "    clf = svm.SVC(C=1e5,probability=True)\n",
    "    clf.fit(X_train, Y_train) \n",
    "    pred = clf.predict_proba(X_test)[:,1]#returns probability estimate of class 1\n",
    "    DF = pd.DataFrame({'prediction': pred, 'true output': Y_test})\n",
    "    return DF\n",
    "\n",
    "# Naive Bayes\n",
    "# Assumes the last column of data is the output dimension\n",
    "def get_pred_nb(train,test):\n",
    "    n,m = train.shape \n",
    "    X_train = train.iloc[:,:m-1] # get training input data\n",
    "    X_test = test.iloc[:,:m-1] # get test input data\n",
    "    Y_train = train.iloc[:,m-1] # get train output data\n",
    "    Y_test = test.iloc[:,m-1] # get test output data\n",
    "    clf = naive_bayes.GaussianNB()\n",
    "    clf.fit(X_train, Y_train) \n",
    "    pred = clf.predict_proba(X_test)[:,1]#returns probability estimate of class 1\n",
    "    DF = pd.DataFrame({'prediction': pred, 'true output': Y_test})\n",
    "    return DF\n",
    "\n",
    "# k-Nearest Neighbor\n",
    "# Assumes the last column of data is the output dimension\n",
    "# Hint: you might want to use predit_proba to get predicted probabilities\n",
    "def get_pred_knn(train,test,k):  \n",
    "    n,m = train.shape \n",
    "    X_train = train.iloc[:,:m-1] # get training input data\n",
    "    X_test = test.iloc[:,:m-1] # get test input data\n",
    "    Y_train = train.iloc[:,m-1] # get train output data\n",
    "    Y_test = test.iloc[:,m-1] # get test output data\n",
    "    clf = neighbors.KNeighborsClassifier(n_neighbors=k)\n",
    "    clf.fit(X_train,Y_train) \n",
    "    pred = clf.predict_proba(X_test)[:,1] #returns probability estimate of class 1\n",
    "    DF = pd.DataFrame({'prediction': pred, 'true output': Y_test})\n",
    "    return DF\n",
    "\n"
